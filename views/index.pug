extends layout

block content
  .row
    h2.sub-header Overview
    .table-responsive
      table.table.table-striped
        thead
          tr
            th #
            th 日期
            th 讀經進度
            th 今日心得
            
        tbody
          each val in data
            tr
              td= val.id
              -var date=new Date(val.date)
              td= date.getFullYear() + '/' + (date.getMonth()+1) + '/' + date.getDate()
              td= val.start+'~'+val.end
              td= val.content